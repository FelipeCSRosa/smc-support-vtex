  "responsive-layout.desktop#links-footer": {
    "children": ["flex-layout.row#links-footer"]
  },
  "flex-layout.row#links-footer": {
    "children": [
      "flex-layout.col#links-footer-contact-us",
      "flex-layout.col#links-footer-help",
      "flex-layout.col#links-footer-institucional"
    ],
    "props": {
      "blockClass": "links-footer",
      "colSizing": "auto"
    }
  },

  "flex-layout.col#links-footer-contact-us": {
    "children": [
      "rich-text#title-link-footer-contact-us",
      "menu#links-footer-contact-us"
    ],
    "props": {
      "preventVerticalStretch": true,
      "blockClass": "footer-links"
    }
  },
  "rich-text#title-link-footer-contact-us": {
    "props": {
      "text": "ATENDIMENTO",
      "blockClass": "title-link-footer"
    }
  },

  "flex-layout.col#links-footer-help": {
