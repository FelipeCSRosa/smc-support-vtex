{
  "responsive-layout.mobile#links-footer": {
    "children": ["flex-layout.col#footer-menu-mobile"]
  },
  "flex-layout.col#footer-menu-mobile": {
    "children": [
      "menu-item#mobile-contact-us",
      "menu-item#mobile-conthelp",
      "menu-item#mobile-institucional"
    ],
    "props": {
      "blockClass": "footer-menu-mobile",
      "preserveLayoutOnMobile": true
    }
  },

  "menu-item#mobile-contact-us": {
    "props": {
      "id": "",
      "type": "custom",
      "blockClass": "link-footer",
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/#",
        "noFollow": false,
        "tagTitle": "ATENDIMENTO",
        "text": "ATENDIMENTO"
      }
    },
    "blocks": ["vtex.menu@2.x:submenu.accordion#contact-us"]
  },
  "vtex.menu@2.x:submenu.accordion#contact-us": {
    "children": ["menu#links-footer-contact-us"],
    "props": {
      "blockClass": "accordion-footer"
    }
  },

  "menu-item#mobile-conthelp": {
